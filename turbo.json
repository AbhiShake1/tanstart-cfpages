{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalDependencies": [
    ".env"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.local"
      ],
      "outputs": [
        "dist"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
